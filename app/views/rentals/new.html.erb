<%= render partial: 'errors', locals: { record: @rental } %>
<%= form_with model: @rental, locals: true do |f| %>
  <table>
    <tr>
      <th><%= f.label :start_date%><th>
      <td><%= f.date_field :start_date%></td>
    </tr>
    <tr>
      <th><%= f.label :end_date%><th>
      <td><%= f.date_field :end_date%></td>
    </tr>
    <tr>
      <th><%= f.label :customer_id%><th>
      <td><%= f.collection_select(:customer_id, @customers, :id, :identification) %></td>
    </tr>
    <tr>
      <th><%= f.label :car_category_id%><th>
      <td><%= f.collection_select(:car_category_id, @car_categories, :id, :name) %></td>
    </tr>
  </table>
  <%= f.submit 'Enviar'%>
<%end%>