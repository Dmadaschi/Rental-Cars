<%= render partial: 'errors', locals: { record: @car_model } %>

<%= form_with model: @car_model, local: true do |f|%>
  <table>
    <tr>
      <td><%= f.label :name %></td>
      <td><%= f.text_field :name %></td>
    </tr>
    <tr>
      <td><%= f.label :year %></td>
      <td><%= f.text_field :year %></td>
    </tr>
    <tr>
      <td><%= f.label :motorization %></td>
      <td><%= f.text_field :motorization %></td>
    </tr>
    <tr>
      <td><%= f.label :fuel_type %></td>
      <td><%= f.text_field :fuel_type %></td>
    </tr>
    <tr>
      <td><%= f.label :manufacturer_id %></td>
      <td><%= f.collection_select(:manufacturer_id, @manufacturers, :id, :name) %></td>
    </tr>
    <tr>
      <td><%= f.label :car_category_id %></td>
      <td><%= f.collection_select(:car_category_id, @car_categories, :id, :name) %></td>
    </tr>
    <tr>
      <td><%= f.submit t('.submit') %></td>
    </tr>
  </table>
<%end%>