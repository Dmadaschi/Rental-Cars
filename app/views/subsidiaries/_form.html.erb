<% @subsidiary.errors.full_messages.each do |message| %>
  <h3><%=message%></h3>
<%end%>
<%= form_with model: @subsidiary do |f|%>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :cnpj %>
  <%= f.text_field :cnpj %>
  <%= f.label :address %>
  <%= f.text_field :address %>
  <%= f.submit 'Enviar'%> 
<%end%>